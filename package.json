{
  "name": "u-miniapp-bridge",
  "version": "1.0.0",
  "description": "通用小程序通信 SDK - WebView 与原生 App 双向通信",
  "author": "Your Name",
  "license": "MIT",
  "type": "module",
  "main": "./dist/u-miniapp-bridge.umd.js",
  "module": "./dist/u-miniapp-bridge.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/u-miniapp-bridge.es.js",
      "require": "./dist/u-miniapp-bridge.umd.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "NATIVE_INTEGRATION.md",
    "ASYNC_OPERATIONS.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run typecheck && npm run build"
  },
  "keywords": [
    "miniapp",
    "webview",
    "bridge",
    "sdk",
    "jsbridge",
    "hybrid",
    "communication",
    "小程序",
    "通信桥接",
    "u-miniapp"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/u-miniapp-bridge.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/u-miniapp-bridge/issues"
  },
  "homepage": "https://github.com/yourusername/u-miniapp-bridge#readme",
  "devDependencies": {
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}
